<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane maxWidth="350.0" prefHeight="200.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tictactoe.controllers.EndGameController">
    <stylesheets>
        <URL value="@../../static/style.css" />
    </stylesheets>

    <Label alignment="CENTER" layoutX="75.0" layoutY="14.0" prefHeight="41.0" prefWidth="200.0" text="Game Over" textAlignment="CENTER" />
    <Label fx:id="winnerTextLabel" alignment="CENTER" layoutX="36.0" layoutY="96.0" prefHeight="41.0" prefWidth="300.0" />
    <Label fx:id="starterTextLabel" alignment="CENTER" layoutX="25.0" layoutY="55.0" prefHeight="41.0" prefWidth="300.0" style="-fx-font-size: 18px;" />
    <Button layoutX="25.0" layoutY="149.0" mnemonicParsing="false" onAction="#continueButtonClick" prefWidth="100.0" text="Continue" />
    <Button layoutX="225.0" layoutY="149.0" mnemonicParsing="false" onAction="#newGameButtonClick" prefWidth="100.0" text="New Game" />
</AnchorPane>
