<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tictactoe.controllers.GameSetUpController">
    <stylesheets>
        <URL value="@../../static/style.css" />
    </stylesheets>

    <Label fx:id="label" alignment="CENTER" layoutY="14.0" prefHeight="50.0" prefWidth="300.0" style="-fx-font-size: 25px !important;" text="Enter Player 1's Name (X)" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" />
    <TextField fx:id="nameInput" layoutX="57.0" layoutY="88.0" onAction="#okButtonClick" prefWidth="187.0" />
    <Button layoutX="14.0" layoutY="150.0" mnemonicParsing="false" onAction="#okButtonClick" prefWidth="125.0" text="Ok" />
   <Button fx:id="playComputerButton" disable="true" layoutX="161.0" layoutY="150.0" mnemonicParsing="false" onAction="#playComputerClick" prefWidth="125.0" text="Play Computer" />
</AnchorPane>
